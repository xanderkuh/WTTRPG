<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
    <TableView fx:id="tableView" prefHeight="400.0" prefWidth="600.0">
        <columns>
            <TableColumn fx:id="textColumn" prefWidth="200.0" text="Text"/>
            <TableColumn fx:id="intColumn" prefWidth="100.0" text="Int"/>
            <TableColumn fx:id="buttonColumn" prefWidth="200.0" text="Buttons"/>
        </columns>
    </TableView>
    <Button fx:id="button1" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" text="Button 1"/>
    <Button fx:id="button2" layoutX="100.0" layoutY="14.0" mnemonicParsing="false" text="Button 2"/>
</AnchorPane>